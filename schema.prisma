datasource db {
    provider = "postgresql"
    url      = env("POSTGRESQL_URL")
}

generator markdown {
    provider = "node ./lib/executable/markdown"
    title    = "Shopping Mall"
}

model inspection_area {
    id String @id @default(uuid())

    /// The geometry of the area
    geog Unsupported("geography(Geometry,4326)")

    /// The JSON properties of the cell, provided at upload time
    properties Json
}
